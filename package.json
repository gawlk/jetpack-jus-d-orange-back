{
  "name": "ensiie-test-back",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@stryker-mutator/babel-transpiler": "^2.1.0",
    "@stryker-mutator/core": "^2.1.0",
    "@stryker-mutator/html-reporter": "^2.1.0",
    "@stryker-mutator/javascript-mutator": "^2.1.0",
    "@stryker-mutator/jest-runner": "^2.1.0",
    "eslint": "^6.5.1",
    "jest": "^24.1.0",
    "newman": "^4.4.1",
    "nodemon": "^1.19.3",
    "stryker-cli": "^1.0.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node app.js --ignore db.json",
    "deamon.start": "pm2 start app.js --interpreter babel-node",
    "deamon.stop": "pm2 stop app.js",
    "deamon.kill": "pm2 kill",
    "test": "jest",
    "test.watch": "jest --watchAll",
    "test.coverage": "jest --coverage",
    "test.mutation": "stryker run",
    "test.file": "babel-node src/testFile.js",
    "test.api": "newman run tests/api/collection.json",
    "test.all": "npm run test && npm run test.coverage && npm run test.mutation && npm run test.api",
    "test.bookingrepo": "babel-node src/testBookingRepo.js"
  },
  "jest": {
    "verbose": true,
    "rootDir": "src",
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "watchPathIgnorePatterns": [
      "db.json"
    ],
    "coveragePathIgnorePatterns": [
      "db.js",
      "index.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.6.3",
    "@babel/polyfill": "^7.6.0",
    "core-js": "^3.2.1",
    "express": "^4.16.4",
    "lowdb": "^1.0.0",
    "pm2": "^3.4.1",
    "uuidv4": "^5.0.1"
  }
}
