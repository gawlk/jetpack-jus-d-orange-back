		{
			"name": "5. Book jetpack",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "29cc937c-0294-49cd-ba3e-31f7b5f41eac",
						"exec": [
							"pm.globals.set('url', 'http://localhost:3000')",
							"",
							"pm.globals.set('booking1.jetpack_id', pm.variables.get('jetpack2.id'));",
							"pm.globals.set('booking1.start_date', '2027-12-26');",
							"pm.globals.set('booking1.end_date', '2027-12-31');"
						],
