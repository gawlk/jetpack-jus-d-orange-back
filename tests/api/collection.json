		{
			"name": "1. Create a jetpack",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "29cc937c-0294-49cd-ba3e-31f7b5f41eac",
						"exec": [
							"",
							"pm.globals.set('url', 'http://localhost:3000')",
							"",
							"pm.globals.set('jetpack1.name', 'Fortnite jetpack');",
							"pm.globals.set('jetpack1.image', 'https://gamepedia.cursecdn.com/fortnite_gamepedia/e/e1/Jetpack_icon.png');",
							"pm.globals.set('jetpack2.name', 'Fortnite jetpack 2');",
							"pm.globals.set('jetpack2.image', 'https://gamepedia.cursecdn.com/fortnite_gamepedia/e/e1/Jetpack_icon.png');"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "4cffee8b-566b-4c46-aa99-e2b76d4487bc",
						"exec": [
							"pm.test(\"Status code is 201\", () => {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Response is JSON\", () => {",
							"    pm.response.json();",
							"});",
							"",
							"pm.test(\"JSON body has new jetpack information\", () => {",
							"    const data = pm.response.json();",
							"    pm.expect(data.name).to.eql(pm.variables.get('jetpack2.name'));",
							"    pm.expect(data.image).to.eql(pm.variables.get('jetpack2.image'));",
							"    pm.globals.set(\"jetpack2.id\", data.id)",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n\t\"name\": \"{{jetpack2.name}}\",\n\t\"image\": \"{{jetpack2.image}}\"\n}"
				},
				"url": {
					"raw": "{{url}}/jetpacks",
					"host": [
						"{{url}}"
					],
					"path": [
						"jetpacks"
					]
				}
			},
			"response": []
		},
