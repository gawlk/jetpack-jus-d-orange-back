							"pm.test(\"Response is JSON\", () => {",
							"    pm.response.json();",
							"});",
							"",
							"pm.test(\"JSON body has booking information\", () => {",
							"    const data = pm.response.json();",
							"",
							"    pm.expect(data.jetpack_id).to.eql(pm.variables.get('booking1.jetpack_id'));",
							"    pm.expect(new Date(data.start_date)).to.eql(new Date(pm.variables.get('booking1.start_date')));",
							"    pm.expect(new Date(data.end_date)).to.eql(new Date(pm.variables.get('booking1.end_date')));",
